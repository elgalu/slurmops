[Unit]
Requires=cloud-init.service reload-kernel-config.service
After=network.target cloud-init.service reload-kernel-config.service
Before=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
Restart=on-failure
ExecStart=/opt/aws/bin/dockercfg.sh

[Install]
WantedBy=multi-user.target
RequiredBy=docker.service
